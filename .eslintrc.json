{
  "extends": "google",
  "env": {
    "browser": false,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "max-len": [1, {"code": 200, "tabWidth": 2, "ignoreUrls": true}],
    "require-jsdoc": 0,
    "max-nested-callbacks": [1, {"max": 8}],
    "no-unused-expressions": 0,
    "no-unused-vars": 2,
    "array-callback-return": 0,
    "handle-callback-err": 0,
    "yoda": 0,
    "quote-props": 0,
    "no-negated-condition": 0,
    "padded-blocks": 0,
    "no-else-return": 0,
    "no-path-concat": 0,
    "key-spacing": 0,
    "no-lonely-if": 0,
    "no-use-before-define": 0,
    "no-caller": 0,
    "no-empty": 0,
    "no-warning-comments": [0, {"terms": ["todo", "fixme", "xxx"], "location": "start"}],
    "comma-dangle": 0,
    "arrow-parens": ["error", "as-needed"]
  }
}
